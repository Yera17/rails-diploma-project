<div class="banner">
<%# image_tag "banner.png", alt: "banner" %>
</div>
<div class="ourcontainer">
 <ul id="genders">
  <% genders = ["Women", "Men"]%>
   <% genders.each do |gender| %>
     <li>
       <%= link_to clothings_path(gender: gender), class: "card-link" do %>
         <div class="card">
         <div class="card-overlay"></div>
           <div class="card-content">
             <%= gender.capitalize %>
           </div>
         </div>
       <% end %>
     </li>
   <% end %>
 </ul>
</div>
